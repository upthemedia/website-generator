    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.1",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^4.1.0",
    "hugo-bin": "^0.55.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
