{
  "name": "websitegenerator",
  "version": "1.0.0",
  "description": "Website Generator",
  "repository": "https://github.com/upthemedia/website-generator.git",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:**",
    "build": "npm run build:webpack && npm run build:hugo",
    "deploy": "cd site && hugo deploy --invalidateCDN  --maxDeletes=-1",
    "start:hugo": "hugo -b http://172.22.0.2:3000 -d site/public  -v -w",
    "start:webpack": "webpack-dev-server --host=172.22.0.2 --config webpack.dev.js --hot",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -b https://www.website.com/ --minify -d site/public -s site -v",
    "build:hugo:preview": "npm run build:hugo  -- -D -F",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
