    "@babel/register": "^7.8.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "assets-webpack-plugin": "^3.9.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
